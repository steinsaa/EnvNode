# MQTT connection
MQTT_HOST=
MQTT_PORT=1883
MQTT_PROTOCOL=mqtt
MQTT_USERNAME=
MQTT_PASSWORD=

# MQTT reconnect/backoff
MQTT_RECONNECT_INITIAL_MS=1000
MQTT_RECONNECT_FACTOR=2
MQTT_RECONNECT_MAX_MS=30000

# Postgres connection
P# Postgres connection
TELEMETRY_STORE=postgres
PGHOST=
PGPORT=5432
PGUSER=envnode_app
PGPASSWORD=
PGDATABASE=envnode

# Postgres tables
POSTGRES_SCHEMA=public
POSTGRES_SENSOR_TABLE=sensor_readings
POSTGRES_STATUS_TABLE=chip_statuses
PGPOOL_MAX=15
PGSSL=false

# Logging
LOG_LEVEL=info

